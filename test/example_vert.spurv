capability Shader
imp = ext_inst_import "GLSL.std.450"
memory_model Logical GLSL450
entry_point Vertex main "main" x vert outColor color

member_decorate gl_PerVertex 0 BuiltIn Position
member_decorate gl_PerVertex 1 BuiltIn PointSize
member_decorate gl_PerVertex 2 BuiltIn ClipDistance
member_decorate gl_PerVertex 3 BuiltIn CullDistance

decorate gl_PerVertex Block
decorate vert Location 0
decorate outColor Location 0
decorate color Location 1

void = type_void
f_void = type_function void
float = type_float 32
vec4 = type_vector float 4
uint = type_int 32 0
uint_1 = constant uint 1
arr_float_uint_1 = type_array float uint_1
gl_PerVertex = type_struct vec4 float arr_float_uint_1 arr_float_uint_1
ptr_Output_gl_PerVertex = type_pointer Output gl_PerVertex
x = variable ptr_Output_gl_PerVertex Output
int = type_int 32 1
int_0 = constant int 0
ptr_Input_vec4 = type_pointer Output vec4
vert = variable ptr_Input_vec4 Input
ptr_Output_vec4 = type_pointer Output vec4
outColor = variable ptr_Output_vec4 Output
color = variable ptr_Input_vec4 Input

main = function void None f_void
lablab = label
ld = load vec4 vert
t0 = access_chain ptr_Output_vec4 x int_0
store t0 ld
t1 = load vec4 color
store outColor t1
return
function_end